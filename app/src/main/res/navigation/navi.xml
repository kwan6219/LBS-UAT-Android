<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navi"
    app:startDestination="@id/firstFragment">

    <fragment
        android:id="@+id/firstFragment"
        android:name="com.mapxus.mapxusmapandroiddemo.examples.integrationcases.explore.FirstFragment"
        android:label="fragment_first"
        tools:layout="@layout/fragment_first" >
        <action
            android:id="@+id/action_firstFragment_to_secondFragment"
            app:destination="@id/secondFragment" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment"
        android:name="com.mapxus.mapxusmapandroiddemo.examples.integrationcases.explore.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" >
        <action
            android:id="@+id/action_secondFragment_to_thridFragment"
            app:destination="@id/thridFragment" />
    </fragment>
    <fragment
        android:id="@+id/thridFragment"
        android:name="com.mapxus.mapxusmapandroiddemo.examples.integrationcases.explore.ThridFragment"
        android:label="fragment_thrid"
        tools:layout="@layout/fragment_thrid" >
        <action
            android:id="@+id/action_thridFragment_to_fourthFragment"
            app:destination="@id/fourthFragment" />
    </fragment>
    <fragment
        android:id="@+id/fourthFragment"
        android:name="com.mapxus.mapxusmapandroiddemo.examples.integrationcases.explore.FourthFragment"
        android:label="fragment_fourth"
        tools:layout="@layout/fragment_fourth" />
</navigation>